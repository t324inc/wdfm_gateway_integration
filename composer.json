{
    "name": "t324inc/wdfm_gateway_integration",
    "description": "Integration with the Gateway server using SOAP",
    "type": "custom-drupal-module",
    "homepage": "https://github.com/t324inc/wdfm_gateway_integration",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "T324",
            "homepage": "https://www.t324.com",
            "role": "Maintainer"
        }
    ],
    "repositories": {
        "t324": {
            "type": "composer",
            "url": "https://t324inc.github.io/satis/"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "~1.0",
        "oomphinc/composer-installers-extender": "~1.0",
        "cweagans/composer-patches": "~1.0",
        "drupal/drupal-library-installer-plugin": "^0.3",
        "meng-tian/async-soap-guzzle": "dev-master",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "enable-patching": true,
        "composer-exit-on-patch-failure": true
    }
}
